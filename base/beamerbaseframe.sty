    \usebeamertemplate{frame begin}%
    \bgroup%
    \aftergroup\beamer@endframe%
    \ifx\@currenvir\beamer@frametext\else\expandafter\let\expandafter\beamer@next\fi}
  \def\beamer@dosingleframe{%
    \bgroup%
    \beamer@inframetrue%
    \usebeamertemplate{frame begin}%
    \bgroup\aftergroup\beamer@endframe%
    \ifx\@currenvir\beamer@frametext\else\expandafter\let\expandafter\beamer@next\fi}
